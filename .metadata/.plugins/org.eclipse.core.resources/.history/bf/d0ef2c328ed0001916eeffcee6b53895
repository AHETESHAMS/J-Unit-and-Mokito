package com.bridgelabz.sortingsofnumbers.util;

public class SortingUtility 
{
	public static int bubbleSort(int array[])
	{
		for(int i=0;i<array.length;i++)
		{
			for(int j=0; j<array.length;j++)
			{
				if(array[j]<array[j+1])
				{
					int temp = array[j];
					array[j] = array[j+1];
					array[j+1] = temp;
				}
			}
		}
		return 0;
	}
}
